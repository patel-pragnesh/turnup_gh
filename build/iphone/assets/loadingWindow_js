for(var images=[],i=1;8>i;i++)images.push("/images/loading/cloud"+i+".png");var startLoading=function(){var i=Ti.UI.createWindow({width:Ti.UI.FILL,height:Ti.UI.FILL,navBarHidden:!0,zIndex:100,backgroundColor:"transparent",opacity:1}),t=Ti.UI.createView({width:Ti.UI.FILL,height:Ti.UI.FILL,opacity:.7,backgroundColor:"#DDD"});i.add(t);var e=Ti.UI.createImageView({images:images,width:"200",height:"200"});if(i.add(e),i.addEventListener("open",function(){e.start()}),i.open(),null!=Alloy.Globals.loadingWindow)try{Alloy.Globals.loadingWindow.close()}catch(o){}Alloy.Globals.loadingWindow=i};exports.startLoading=startLoading;var endLoading=function(){var i=Alloy.Globals.loadingWindow;setTimeout(function(){try{i.close()}catch(t){}},200)};exports.endLoading=endLoading;